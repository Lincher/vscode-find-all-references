{
    "name": "referenceshelper",
    "displayName": "referenceshelper",
    "description": "Find all references for almost all files.",
    "version": "0.0.1",
    "publisher": "gayanhewa",
    "engines": {
        "vscode": "^1.13.0"
    },
    "keywords": [
        "php", "ruby", "rust", "python", "erlang", "java", "cpp", "c", "markdown", "xml", "haml", "twig", "bat",
        "bibtex", "clojure", "coffeescript", "css", "csharp", "diff", "dockerfile", "fsharp", "go", "groovy", "handlebars", "html",
        "ini", "json", "latex", "less", "lua", "makefile", "objective-c", "objective-cpp", "perl6", "perl", "powershell",
        "jade", "python", "r", "razor", "scss", "shaderlab", "shellscript", "sql", "swift", "tex", "xsl", "yaml",
        "intellisense",
        "goto",
        "findreferences",
        "help",
        "navigation"
    ],
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:php",
        "workspaceContains:*.json",
        "workspaceContains:*.lock"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "configuration": {
            "properties": {
                "find.all.references.engine": {
                    "default":"rg"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
